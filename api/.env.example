APP_NAME=Skydivemanifest
APP_ENV=production
APP_LOCALE=en
APP_KEY=
APP_DEBUG=false
APP_URL=http://localhost
APP_URL_FRONTEND=http://localhost/frontend
APP_DELETE_INACTIVE_USERS=120
